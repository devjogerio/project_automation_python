{
  "exemplo_noticias": {
    "name": "exemplo_noticias",
    "url": "https://example-news.com",
    "enabled": true,
    "scraping_type": "static",
    "selectors": {
      "titulos": "h1, h2, h3",
      "artigos": "article",
      "datas": "time[datetime]",
      "autores": ".author, .byline"
    },
    "wait_for": null,
    "schema": null,
    "schedule": "0 9 * * *",
    "max_pages": 5,
    "respect_robots_txt": true,
    "custom_headers": null
  },
  "exemplo_produtos": {
    "name": "exemplo_produtos",
    "url": "https://example-shop.com",
    "enabled": true,
    "scraping_type": "dynamic",
    "selectors": {
      "produtos": ".product-item",
      "precos": ".price",
      "nomes": ".product-name",
      "imagens": ".product-image img"
    },
    "wait_for": ".product-grid",
    "schema": null,
    "schedule": "0 12 * * 1",
    "max_pages": 3,
    "respect_robots_txt": true,
    "custom_headers": null
  }
}